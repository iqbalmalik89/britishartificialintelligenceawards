	function fullScreenSlide() {
		var browserheight = jQuery(window).height();
		var browserWidth = jQuery(window).width();
		var halfWidth = browserWidth / 2;
		var threeQuarterWidth = browserWidth * .75;	
		

		
		
		$headerHeight = jQuery('#absolute-to-fixed-bar').height() - 4;
		$newHeight = browserheight - $headerHeight;
		$fullVideoHeight = $newHeight + 200;
		
		jQuery('.fill-browser').css('min-height', $newHeight);
		jQuery('.fill-browser.background-video').css('min-height', $fullVideoHeight);
		
		var width = jQuery(window).width();
		if (width >= 780 && jQuery('body').hasClass('wide-enough-for-sidebar')) {
			jQuery('#services-info-sidebar').css('height', $newHeight);
		} else {
			jQuery('#services-info-sidebar').css('height', 'auto');
		}
			
		
		
		jQuery('body').addClass('fullscreen-sections');
		
		jQuery('#content').imagesLoaded(function() {
			jQuery('body').addClass('imagesloaded');
			jQuery(".fill-browser-inner").each(function() {
					var $thisinner = jQuery(this).outerHeight();
					var $extraspace = ( ($newHeight - $thisinner) / 2 );
					
					if ($extraspace > 0){
						jQuery(this).css('margin-top', $extraspace);
					}
					
					
			});
			
			jQuery(".fill-browser-inner-section4").each(function() {
					var $thisinner = jQuery(this).height();
					var $extraspace = ( (browserheight - $thisinner) / 2 ) - $headerHeight;
					
					if ($extraspace > 0){
						jQuery(this).css('margin-top', $extraspace);
					}
					
					
			});
	
		});
		
	
		var $recentImageHeight = jQuery('.recent-image').outerHeight(true);
		
